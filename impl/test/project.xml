<?xml version="1.0"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <extend>../../project.xml</extend>
  <name>sakai-assignment2-impl-test</name>
  <groupId>sakaiproject</groupId>
  <artifactId>sakai-assignment2-test</artifactId>
  <organization>
    <name>Sakai Project</name>
    <url>http://www.sakaiproject.org/</url>
  </organization>
  <inceptionYear>2006</inceptionYear>
<!-- the test should be deployed as a jar,
		this has to be here so the logic and dao tests can share a class -->
  <properties>
		<!-- deploy as a jar -->
		<deploy.type>jar</deploy.type>
	</properties>
  <dependencies>
<!-- Sakai dependencies -->
<!-- Assignment2 dependencies -->
    <dependency>
      <groupId>sakaiproject</groupId>
      <artifactId>sakai-assignment2-model</artifactId>
      <version>${sakai.version}</version>
    </dependency>
    <dependency>
      <groupId>sakaiproject</groupId>
      <artifactId>sakai-assignment2-logic-api</artifactId>
      <version>${sakai.version}</version>
    </dependency>
<!-- generic DAO -->
    <dependency>
         <groupId>generic-dao</groupId>
         <artifactId>generic-dao</artifactId>
         <version>0.9</version>
      </dependency>
<!-- Spring and Hibernate dependencies. -->
    <dependency>
      <groupId>${sakai.spring.groupId}</groupId>
      <artifactId>${sakai.spring.artifactId}</artifactId>
      <version>${sakai.spring.version}</version>
    </dependency>
<!-- Apache commons dependencies -->
    <dependency>
      <groupId>commons-logging</groupId>
      <artifactId>commons-logging</artifactId>
      <version>1.0.4</version>
    </dependency>
  </dependencies>
  <build>
    <resources/>
    <sourceDirectory>src/java</sourceDirectory>
  </build>
</project>
