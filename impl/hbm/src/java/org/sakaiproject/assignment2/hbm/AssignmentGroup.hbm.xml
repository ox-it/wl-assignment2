<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.sakaiproject.assignment2.model.AssignmentGroup" table="A2_ASSIGN_GROUP_T">
       
        <id name="assignmentGroupId" column="ASSIGNMENT_GROUP_ID" type="long">
					<generator class="native">
						<param name="sequence">A2_ASSIGN_GROUP_S</param>
					</generator>
				</id>
        
        <version name="version" type="integer" column="VERSION"/>

        <!-- The Authz groupId -->
        <property name="groupId" type="java.lang.String" unique-key="ASSIGN_ID_GROUP_ID">
            <column name="GROUP_ID" not-null="true" length="255" />
        </property>
              
    </class>
</hibernate-mapping>