<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" 
	"http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

   <!-- rich text evolver bean def -->
   <bean id="richTextEvolver" class="org.springframework.beans.factory.config.BeanReferenceFactoryBean">
      <property name="targetBeanName" value="sakaiFCKTextEvolver" />
   </bean>

	<!-- Renderers -->
   <bean id="NavbarRenderer" class="org.sakaiproject.assignment2.tool.producers.NavBarRenderer" />
   <bean id="PagerRenderer" class="org.sakaiproject.assignment2.tool.producers.PagerRenderer">
      <property name="externalLogic" ref="org.sakaiproject.assignment2.logic.ExternalLogic" />
	  <property name="pagerBean" ref="PagerBean" />
      <property name="messageLocator" ref="messageLocator" />
   </bean>
   <bean id="SortHeaderRenderer" class="org.sakaiproject.assignment2.tool.producers.SortHeaderRenderer" />
   <bean id="AttachmentListRenderer" class="org.sakaiproject.assignment2.tool.producers.AttachmentListRenderer">
   	  <property name="contentHostingService" ref="org.sakaiproject.content.api.ContentHostingService" />
   	  <property name="externalLogic" ref="org.sakaiproject.assignment2.logic.ExternalLogic" />
   	  <property name="messageLocator" ref="messageLocator" />
   </bean>
   

   <!-- list the producer beans here -->
   <bean class="org.sakaiproject.assignment2.tool.producers.AddAttachmentHelperProducer">
      <property name="sessionManager" ref="org.sakaiproject.tool.api.SessionManager" />
   </bean>
   
   <bean class="org.sakaiproject.assignment2.tool.producers.AjaxCallbackProducer">
	  <property name="assignmentLogic" ref="org.sakaiproject.assignment2.logic.AssignmentLogic" />
      <property name="externalLogic" ref="org.sakaiproject.assignment2.logic.ExternalLogic" />
      <property name="sessionManager" ref="org.sakaiproject.tool.api.SessionManager" />
   </bean>	

   <bean class="org.sakaiproject.assignment2.tool.producers.AssignmentAddProducer">
      <property name="navBarRenderer" ref="NavbarRenderer" />
      <property name="richTextEvolver" ref="richTextEvolver" />
      <property name="messageLocator" ref="messageLocator" />
      <property name="dateEvolver" ref="fieldDateInputEvolver" />
      <property name="assignmentLogic" ref="org.sakaiproject.assignment2.logic.AssignmentLogic" />
      <property name="externalLogic" ref="org.sakaiproject.assignment2.logic.ExternalLogic" />
      <property name="externalGradebookLogic" ref="org.sakaiproject.assignment2.logic.ExternalGradebookLogic" />
      <property name="previewAssignmentBean" ref="PreviewAssignmentBean" />
      <property name="locale" ref="requestLocale" />
      <property name="sessionManager" ref="org.sakaiproject.tool.api.SessionManager" />
      <property name="assignment2EntityBeanLocator" ref="Assignment2"/>
      <property name="attachmentListRenderer" ref="AttachmentListRenderer" />
   </bean>

   <bean class="org.sakaiproject.assignment2.tool.producers.AssignmentAddGradebookItemProducer">
      <property name="navBarRenderer" ref="NavbarRenderer" />
      <property name="messageLocator" ref="messageLocator" />
      <property name="dateEvolver" ref="fieldDateInputEvolver" />
   </bean>

   <bean class="org.sakaiproject.assignment2.tool.producers.AssignmentPreviewProducer">
      <property name="navBarRenderer" ref="NavbarRenderer" />
      <property name="messageLocator" ref="messageLocator" />
      <property name="assignmentLogic" ref="org.sakaiproject.assignment2.logic.AssignmentLogic" />
      <property name="externalLogic" ref="org.sakaiproject.assignment2.logic.ExternalLogic" />
      <property name="previewAssignmentBean" ref="PreviewAssignmentBean" />
      <property name="locale" ref="requestLocale" />
      <property name="attachmentListRenderer" ref="AttachmentListRenderer" />
   </bean>
   
   <bean class="org.sakaiproject.assignment2.tool.producers.AssignmentGradeAssignmentProducer">
      <property name="navBarRenderer" ref="NavbarRenderer" />
      <property name="pagerRenderer" ref="PagerRenderer" />
      <property name="messageLocator" ref="messageLocator" />
      <property name="pagerBean" ref="PagerBean" />
      <property name="assignmentLogic" ref="org.sakaiproject.assignment2.logic.AssignmentLogic" />
      <property name="messages" ref="targettedMessageList" />
      <property name="externalLogic" ref="org.sakaiproject.assignment2.logic.ExternalLogic" />
      <property name="locale" ref="requestLocale" />
      <property name="sortHeaderRenderer" ref="SortHeaderRenderer" />
      <property name="attachmentListRenderer" ref="AttachmentListRenderer" />
   </bean>
   
   <bean class="org.sakaiproject.assignment2.tool.producers.AssignmentGradeReportProducer">
      <property name="navBarRenderer" ref="NavbarRenderer" />
      <property name="pagerBean" ref="PagerBean" />
      <property name="messageLocator" ref="messageLocator" />
   </bean>
   
   <bean class="org.sakaiproject.assignment2.tool.producers.AssignmentListReorderProducer">
      <property name="navBarRenderer" ref="NavbarRenderer" />
	  <property name="pagerRenderer" ref="PagerRenderer" />
	  <property name="pagerBean" ref="PagerBean" />
      <property name="messageLocator" ref="messageLocator" />
      <property name="assignmentLogic" ref="org.sakaiproject.assignment2.logic.AssignmentLogic" />
      <property name="externalLogic" ref="org.sakaiproject.assignment2.logic.ExternalLogic" />
      <property name="locale" ref="requestLocale" />
   </bean>

   <bean class="org.sakaiproject.assignment2.tool.producers.AssignmentListSortViewProducer">
      <property name="navBarRenderer" ref="NavbarRenderer" />
      <property name="messageLocator" ref="messageLocator" />
      <property name="externalLogic" ref="org.sakaiproject.assignment2.logic.ExternalLogic" />
      <property name="pagerBean" ref="PagerBean" />
      <property name="assignmentLogic" ref="org.sakaiproject.assignment2.logic.AssignmentLogic" />
      <property name="pagerRenderer" ref="PagerRenderer" />
      <property name="locale" ref="requestLocale" />
      <property name="assignment2Bean" ref="Assignment2Bean" />
      <property name="sortHeaderRenderer" ref="SortHeaderRenderer" />
   </bean>

   <bean class="org.sakaiproject.assignment2.tool.producers.FinishedHelperProducer">
   </bean>

   <bean class="org.sakaiproject.assignment2.tool.producers.GradebookAddItemProducer">
	  <property name="externalLogic" ref="org.sakaiproject.assignment2.logic.ExternalLogic" />
      <property name="sessionManager" ref="org.sakaiproject.tool.api.SessionManager" />
      <property name="messageLocator" ref="messageLocator" />
   </bean>
   
   <bean class="org.sakaiproject.assignment2.tool.producers.PermissionsProducer">
	  <property name="externalLogic" ref="org.sakaiproject.assignment2.logic.ExternalLogic" />
      <property name="sessionManager" ref="org.sakaiproject.tool.api.SessionManager" />
      <property name="messageLocator" ref="messageLocator" />
   </bean>
   
      <bean class="org.sakaiproject.tool.producers.StudentAssignmentListProducer">
      <property name="externalLogic" ref="org.sakaiproject.assignment2.logic.ExternalLogic" />
      <property name="assignmentLogic" ref="org.sakaiproject.assignment2.logic.AssignmentLogic" />
      <property name="pagerRenderer" ref="PagerRenderer" />
      <property name="locale" ref="requestLocale" />
      <property name="assignment2Bean" ref="Assignment2Bean" />
      <property name="sortHeaderRenderer" ref="SortHeaderRenderer" />
      <property name="pagerBean" ref="PagerBean" />
   </bean>
   
   <bean class="org.sakaiproject.assignment2.tool.producers.StudentSubmitProducer">
      <property name="richTextEvolver" ref="richTextEvolver" />
      <property name="messageLocator" ref="messageLocator" />
      <property name="assignmentLogic" ref="org.sakaiproject.assignment2.logic.AssignmentLogic" />
      <property name="externalLogic" ref="org.sakaiproject.assignment2.logic.ExternalLogic" />
      <property name="externalGradebookLogic" ref="org.sakaiproject.assignment2.logic.ExternalGradebookLogic" />
      <property name="previewAssignmentBean" ref="PreviewAssignmentBean" />
      <property name="locale" ref="requestLocale" />
      <property name="sessionManager" ref="org.sakaiproject.tool.api.SessionManager" />
      <property name="assignment2EntityBeanLocator" ref="Assignment2"/>
      <property name="attachmentListRenderer" ref="AttachmentListRenderer" />
      <property name="assignmentSubmissionBeanLocator" ref="AssignmentSubmission" />
   </bean>

   <!--  AJAX Fragment Producers -->
   <bean class="org.sakaiproject.assignment2.tool.producers.FragmentAttachmentsProducer">
   	  <property name="assignment2EntityBeanLocator" ref="Assignment2"/>
   	  <property name="contentHostingService" ref="org.sakaiproject.content.api.ContentHostingService" />
   	  <property name="sessionManager" ref="org.sakaiproject.tool.api.SessionManager" />
   	  <property name="externalLogic" ref="org.sakaiproject.assignment2.logic.ExternalLogic" />
   	  <property name="messageLocator" ref="messageLocator" />
   	  <property name="attachmentListRenderer" ref="AttachmentListRenderer" />
   </bean>
  
   
   <!--  Beans -->
   <bean id="PagerBean" class="org.sakaiproject.assignment2.tool.beans.PagerBean" />
   
   <bean id="Assignment2Bean" class="org.sakaiproject.assignment2.tool.beans.Assignment2Bean">
   	  <property name="logic" ref="org.sakaiproject.assignment2.logic.AssignmentLogic" />
   	  <property name="externalLogic" ref="org.sakaiproject.assignment2.logic.ExternalLogic" />
   	  <property name="externalAnnouncementLogic" ref="org.sakaiproject.assignment2.logic.ExternalAnnouncementLogic" />
   	  <property name="messages" ref="targettedMessageList"/>
   	  <property name="assignment2EntityBeanLocator" ref="Assignment2"/>
   	  <property name="previewAssignmentBean" ref="PreviewAssignmentBean" />
   	  <property name="messageLocator" ref="messageLocator" />
   	  <property name="sessionManager" ref="org.sakaiproject.tool.api.SessionManager" />
   </bean>
   
   <bean id="GradebookItemBean" class="org.sakaiproject.assignment2.tool.beans.GradebookItemBean">
      <property name="externalLogic" ref="org.sakaiproject.assignment2.logic.ExternalLogic" />
      <property name="externalGradebookLogic" ref="org.sakaiproject.assignment2.logic.ExternalGradebookLogic" />
   	  <property name="messages" ref="targettedMessageList"/>
   	  <property name="messageLocator" ref="messageLocator" />
   </bean>
   
   <bean id="PreviewAssignmentBean" class="org.sakaiproject.assignment2.tool.beans.PreviewAssignmentBean" />

   <bean id="FilePickerBean" class="org.sakaiproject.assignment2.tool.beans.FilePickerBean">
   	  <property name="contentHostingService" ref="org.sakaiproject.content.api.ContentHostingService" />
      <property name="sessionManager" ref="org.sakaiproject.tool.api.SessionManager" />
      <property name="assignmentLogic" ref="org.sakaiproject.assignment2.logic.AssignmentLogic" />
      <property name="assignment2EntityBeanLocator" ref="Assignment2"/>
   </bean>
	
	<!-- Validators -->
	<bean id="Assignment2Validator" class="org.sakaiproject.assignment2.tool.beans.Assignment2Validator" />

</beans>